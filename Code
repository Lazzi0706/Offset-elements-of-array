const value = 6; // Эта хрень число элементов в массиве
let arr = []; // Эта хрень — пустой массив
for (let i = 0;i<value;i++){ // Эта хрень — цикл, который заполняет массив числами по порядку, от 1 до 6
  arr[i] = i; // То что в квадратных скобках — сколько элементов в данном масиве, то что после знака "=" — значение, которое они принимают, т.е постоянно на +1
  for (let j = 0;j<value;j++){ // Эта хрень реверсирует массив. Push добавляет новый элемент в массив с прибавлением на один; Shift - сносит нахуй 0 элемент массива, тем самым проталкивая все другие дальше 
    arr.push(j+1);
    arr.shift();
};
};
console.log(arr); // Выводит это всё в консоль
